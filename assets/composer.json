{
  "name": "idriftweb/project-name",
  "type": "project",
  "description": "",
  "authors": [
    {
      "name": "Magnus Hauge Bakke",
      "email": "magnus@idrift.no"
    },
    {
      "name": "Daniel Rufus Kaldheim",
      "email": "daniel@idrift.no"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "composer/installers": "~1.0",
    "fancyguy/webroot-installer": "1.1.0",
    "wordpress/wordpress": ">=4.1",
    "wpackagist-plugin/wordpress-seo": ">=1.4.24",
    "wpackagist-plugin/ewww-image-optimizer": ">=1.8.2",
    "wpackagist-plugin/w3-total-cache": ">=0.9.3",
    "idriftweb/wp-template": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    },
    "webroot-dir": "wordpress",
    "webroot-package": "wordpress/wordpress"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "http://packages.idriftweb.no"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
